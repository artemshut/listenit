<h1>All users</h1>
<ul class="users">
<% if admin_signed_in? %>
  <% @users.each do |user| %>
        <%= user.name%>
          <%= link_to t(:delete), user_path(user), :method => :delete, :confirm => "You sure?",
                      :title => "Delete #{user.name}" %>
  <% end %>
  <% end %>
</ul>