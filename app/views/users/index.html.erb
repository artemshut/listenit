<h1>All users</h1>
<ul class="users">
  <% if admin_signed_in? %>
      <table>
      <% @users.each do |user| %>
          <tr>
        <td><%= user.name%>
          <%= link_to t(:delete), user_path(user), :method => :delete, :confirm => "You sure?",
                      :title => "Delete #{user.name}" %> </td> </tr>
    <% end %>

  <% end %>
  </table>
</ul>